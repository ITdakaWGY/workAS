@font-face {
  font-family: 'iconfont';  /* project id 1639950 */
  src: url('//at.alicdn.com/t/font_1639950_yw95xmmp5sl.eot');
  src: url('//at.alicdn.com/t/font_1639950_yw95xmmp5sl.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1639950_yw95xmmp5sl.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1639950_yw95xmmp5sl.woff') format('woff'),
  url('//at.alicdn.com/t/font_1639950_yw95xmmp5sl.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1639950_yw95xmmp5sl.svg#iconfont') format('svg');
}
.package_div {
	width: 60%;
}

.iconfont{
	font-family: 'iconfont';
}

.w1200 {
	width:1200px;
	margin: 0 auto;
	height: auto
}

.w1200:after {
	content: "";
	width: 0;
	height: 0;
	display: block;
	clear: both
}

.ai_card {
	width: 100%;
	height: auto;
	overflow: hidden;
	padding-bottom: 30px;
	margin-top: 30px;
}
.iconSize{
	font-size: 75px;
	color: #00b8ff;
}
.ai_card h2.t {
	padding-top:20px;
	font-size: 36px;
	text-align: center;
	font-weight: 400
}

.ai_card p.sub_t {
	padding-top: 15px;
	font-size: 18px;
	text-align: center;
	padding-bottom: 30px;
	color: #00B2EE;
}

.ai_card ul {
	padding-bottom: 30px
}

.ai_card ul li {
	float: left;
	position: relative;
	width: 25%;
	height: 180px;
	border: 1px solid #e7e7e7;
	margin-left: -1px;
	z-index: 10;
	margin-right: 12.5%;
	box-sizing: border-box;
	cursor: pointer;
	margin-bottom: 30px;
	background:#aedfff;
	background-size:100% 100%;
	border-radius:6px ;
}

.ai_card ul li.active{
	box-shadow: 0 2px 20px rgba(175, 175, 175, .3);
}

.ai_card ul li:hover{
	color: #ffffff !important;
}
.ai_card ul li.active:before {
	width: 100%;
}

.ai_card ul li:nth-child(3n){
	margin-right: 0;
}
.ai_card ul li:before {
	content: "";
	width: 0;
	height: 2px;
	background: #00b8ff;
	top: 0;
	left: 0;
	position: absolute;
	z-index: 999;
	transition: 1s;
}

.ai_card ul li .info {
	position: absolute;
	top: 10px;
	width: 100%;
	left: 0;
	transition: .5s
}

.ai_card ul li .info .img {
	width: 100%;
	height: 90px;
	text-align: center
}

.ai_card ul li .info h3 {
	font-weight: 400;
	color: #00b8ff;
	padding-bottom: 25px;
	font-size: 24px;
	text-align: center
}

.ai_card ul li .info p {
	width: 80%;
	height: 85px;
	line-height: 24px;
	margin: 0 auto;
	color: #73747b;
	font-size: 14px
}

.ai_card ul li a.btn {
	position: absolute;
	width: 120px;
	height: 40px;
	line-height: 40px;
	display: block;
	left: 50%;
	margin-left: -60px;
	bottom: 30px;
	color: #fff;
	background:#3E8BCC;
	opacity: 0;
	border-radius: 2px;
	transition: .5s;
	font-size: 16px;
	text-align: center
}


.ai_card ul li.active .info {
	top: 0px;
	transition: .5s;
	-moz-transition: .5s;
	-webkit-transition: .5s;
	-o-transition: .5s
}

.ai_card ul li.active a.btn {
	opacity: 1
}
.t{
	color: #00B2EE;
}

.color-bai{
	color: #fff !important;
}

.bg-lan{
	background:rgba(1,144,255,0.8);
}

.footer{
	z-index: 9999;
}
@media only screen and (min-width: 1400px) {
}

.indexBanner{
	background: rgba(241,241,241,0.5)
}
.layui-carousel{
	height: 350px !important;
	margin: 0px auto;
	width: 1200px !important;
	
}
.layui-carousel img{
	width: 100%;
	max-width: 1920px;
	height: 380px;
}
.listNav{
	overflow: hidden;
	
}
