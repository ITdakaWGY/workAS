/* BB */

.layui-table-rightBB {
	width: calc(100% - 260px) !important;
	float: right;
}

.leftTiJianBB {
	width: 250px;
	display: inline-block;
	float: left;
	margin-right: 10px;
	background: #f5f5f5;
	margin-top: 10px;
	position: relative;
}

.leftTiJianBB>p {
	width: 50%;
	float: left;
	text-align: center;
	line-height: 45px;
	position: relative;
	background: #e5e5e5;
	cursor: pointer;
}

.leftTiJianBB>p:first-child {
	border-right: 1px solid #fff;
	box-sizing: border-box;
}

.leftTiJianBB>ul {
	width: 50%;
	float: left;
	padding: 8px 0;
	box-sizing: border-box;
}

.leftTiJianBB::before {
	width: 1px;
	height: 100%;
	top: 0;
	bottom: 0;
	left: 49.5%;
	background: #fff;
	box-sizing: border-box;
	content: '';
	position: absolute;
}

.leftTiJianBB li {
	line-height: 35px;
	padding-left: 18px;
	margin-left: 8px;
}

.leftTiJianBB ul>.active {
	display: inline-block;
	background: #fff;
	height: 35px;
	padding-right: 18px;
	border-radius: 80px;
}

.detaiLeft {
	float: left;
	width: 20%;
	height: 100%;
}

.detailRight {
	width: 82%;
	position: fixed;
	left: 18%;
	box-sizing: border-box;
	height: 100%;
}


