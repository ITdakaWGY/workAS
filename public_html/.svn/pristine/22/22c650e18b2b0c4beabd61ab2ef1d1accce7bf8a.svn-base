//获取多选框的值
function splicingCheckbox(name) {
	obj = document.getElementsByName(name);
	check_val = [];
	for (k in obj) {
		if (obj[k].checked)
			check_val.push(obj[k].value);
	}
	return check_val;

}



function base64Url(img){
		$.ajax({
				type: "post",
				url: BaseUrl + 'grjbxx/base64Url',
				async: false,
				data: img,
                processData: false,
                contentType: "application/json", 
				success: function(data) {
					if(data!=null){
						$('#url').val(data);
					}else{
						alert("上传失败,请联系管理员")
					}
					
				},
				error: function(data) {
					alert("上传失败,请联系管理员")
				}
			});
}
