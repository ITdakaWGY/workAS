        <!--症状信息已体检 -->
        <if test="tjflagzz==1">
            and tjflagzz = #{tjflagzz}
        </if>
        <!--症状信息未体检 -->
        <if test="tjflagzz==0">
            and (tjflagzz IS NULL or tjflagzz='')
        </if>

        <!--外科已体检 -->
