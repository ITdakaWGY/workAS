        </if>
        <!--神经科未体检 -->
        <if test="tjflagsjk==0">
            and (tjflagsjk IS NULL or tjflagsjk='')
        </if>


        <!--个人已体检 -->
        <if test="tjbs==1">
            and tjbs = #{tjbs}
        </if>
        <!--个人未体检 -->
        <if test="tjbs==0">
            and (tjbs IS NULL or tjbs='')
        </if>
        <!--体检统计 -->
        <if test="res1==2">
            and (res1 IS NULL or res1='')
        </if>
        <if test="res1==1">
            and res1 = "1"
        </if>

    </select>




</mapper>